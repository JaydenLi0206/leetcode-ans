class Solution {
    public boolean isPalindrome(int x) {
        int l, r;
        String s = new String(Integer.toString(x));
        for(l = 0 , r = s.length() - 1; l < r; l++, r--){
            if(s.charAt(l) != s.charAt(r)){
                return false;
            }
        }
        return true;
    }
}
